<?php
// src/MisMed/MediaBundle/Entity/Tag_news.php
namespace MisMed\MediaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="tag_news")
 */
class Tag_news
{
    /**
     * @ORM\Column(type="integer")
     */
    protected $id_tag;

    /**
     * @ORM\Column(type="integer")
     */
    protected $id_news;

}
